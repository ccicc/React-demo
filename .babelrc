{
    "presets":[
        ["env",{
            "targets": {
                "browsers": ["ie >= 9", "last 2 Chrome versions"],
                "node": "current"
            },
            "loose": true
        }],
        "react",
        "stage-0",
        "flow"
    ],
    "plugins":[
        ["transform-runtime",{
            "helpers": true,
            "polyfill": true,
            "regenerator": true,
            "modulename": "babel-runtime"
        }],
        ["import",{
            "style": "css",
            "libraryName": "antd-mobile"
        }],
        "istanbul"
    ]
}
